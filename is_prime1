#6k+-1

def is_prime(my_num):

    if my_num == 2 or my_num == 3 or my_num == 5:return True
    if my_num % 2 == 0 or my_num % 3 == 0 or my_num % 5 == 0:return False

    sqr=int(math.sqrt(my_num))
            
    for x in range(6, sqr+2, 6):
        if my_num % (x-1) == 0 or my_num % (x+1) == 0:
            return False
    return True
